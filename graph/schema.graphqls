enum ModelType {
  HUGGING_FACE
  EXTERNAL
}

type HuggingFaceConfig {
  organization: String!
  model: String!
}

type Model {
  id: ID!
  type: ModelType!
  huggingFaceModel: HuggingFaceConfig
}


type Query {
  models: [Model!]!
}

input HuggingFaceConfigInput {
  organization: String!
  model: String!
}

input AddModelInput {
  type: ModelType!
  huggingFaceModel: HuggingFaceConfigInput
}

type Mutation {
  addModel(input: AddModelInput!): Model!
}
